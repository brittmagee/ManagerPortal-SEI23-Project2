<div class="parent">
<div class="profile">
    <div class="pic">
        <img src="/hoopes.jpg" class="exampleProfile">
        <h2>Lisa Hoopes,  Ph.D.</h2>
        <h4>Director of Research | Conservation and Nutrition</h4>
    </div>
    <div>
        <p>&#9993; lhoopes@example.com</p>
        <p>&#9742; 404-555-7289</p>
        <p>&#9998; Edit</p>
    </div>
</div>
<div class="allMovesContent">
<h2>All Feedback</h2>
<form method="GET" action="/feedback/new">
    <input type="submit" vvalue="+" class="newMoveButton"/>
</form>
<table>
    <thead>
        <tr>
            <th>Date</th>
            <th>Type</th>
            <th>Title</th>
            <th>Description</th>
            <th>Staff Needed</th>
            <th>Status</th>
            <th>Priority</th>
        </tr>
    </thead>
    <tbody>
        {{#each allFeedback}}
        <tr>
            <td><a href ="feedback/{{_id}}">{{date}}</a></td>
            <td>{{type}}</td>
            <td>{{title}}</td>
            <td>{{description}}</td>
            <td>{{staff}}</td>
            <td>{{status}}</td>
            <td>{{priority}}</td>
        </tr>
     {{/each}}
     </tbody>
</table>
</div>
</div> 