<div class="parent">
<div class="profile">
    <div class="pic">
        <img src="/rivera.jpg" class="exampleProfile">
        <h2>Anthony Rivera</h2>
        <h4>Vice President | Guest Experience & Hospitality</h4>
    </div>
    <div>
        <p>&#9993; arivera@example.com</p>
        <p>&#9742; 404-555-9728</p>
        <p>&#9998; Edit</p>
    </div>
</div>
<div class="allMovesContent">
<h2>All Upcoming Events</h2>
<form method="GET" action="/events/new">
    <input type="submit" value="+" class="newMoveButton"/>
</form>
<table>
    <thead>
        <tr>
            <th>Date</th>
            <th>Type</th>
            <th>Attendees</th>
            <th>Title</th>
            <th>Description</th>
            <th>Location</th>
            <th>Staff Required</th>
        </tr>
    </thead>
    <tbody>
        {{#each allAquariumEvents}}
        <tr>
            <td><a href ="events/{{_id}}">{{date}}</a></td>
            <td>{{type}}</td>
            <td>{{attendees}}</td>
            <td>{{title}}</td>
            <td>{{description}}</td>
            <td>{{location}}</td>
            <td>{{staff}}</td>
        </tr>
     {{/each}}
     </tbody>
</table>
</div>
</div> 